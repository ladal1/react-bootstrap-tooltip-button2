  "dependencies": {
    "react": "^16.0.1",
    "react-bootstrap": "^1.5.0",
    "react-dom": "^16.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
