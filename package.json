{
  "name": "react-bootstrap-tooltip-button",
  "version": "1.0.6",
  "description": "React-Bootstrap button with Tooltip when disabled",
  "main": "lib/TooltipButton.js",
  "author": "Dennis Bemmann",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:pastaclub/react-bootstrap-tooltip-button"
  },
  "scripts": {
    "compile-js": "./node_modules/.bin/babel --plugins transform-react-jsx --presets es2015,stage-0 -d lib/ src/",
    "compile-sass": "./node_modules/node-sass/bin/node-sass src --output lib",
    "compile": "npm run compile-js && npm run compile-sass",
    "clean": "rm -r lib",
    "prepublish": "npm run compile"
  },
  "peerDependency": {
    "react": ">= 15.4.2 < 17.0.0",
    "react-bootstrap": ">= 0.30.8 < 0.33.0",
    "react-dom": ">= 15.4.2 < 17.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "node-sass": "^4.5.1"
  }
}
